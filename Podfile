platform:ios, '7.0'

target 'Bugatti' do

pod 'Masonry',              '~> 1.0.2'
pod 'AFNetworking',         '~> 3.1.0'
pod 'SDWebImage',           '~> 4.0.0'
pod 'IQKeyboardManager',    '~> 3.3.7'

#pod 'CorePlot'



#shareSDK主模块
#
#
pod 'ShareSDK3' ,           '~> 3.6.3'

# UI模块(非必须，需要用到ShareSDK提供的分享菜单栏和分享编辑页面需要以下1行)
pod 'ShareSDK3/ShareSDKUI'

#公共库
pod 'MOBFoundation'

#平台SDK模块
pod 'ShareSDK3/ShareSDKPlatforms/WeChat'

#扩展模块（在调用可以弹出我们UI分析方法的时候是必需的）
pod 'ShareSDK3/ShareSDKExtension'


#pod 'TBBaseApp',:git => 'https://github.com/james1896/TBBaseApp',:tag=>"0.0.6"
pod 'TBBaseApp',:git => 'https://github.com/james1896/TBBaseApp'
end
